<div class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="header">
                                <h4 class="title">Add Product</h4>
                            </div>
                            <div class="content">
                                <div class="payment-list">
                                    <form action="/admin/create-product" id="add_product_form" enctype="multipart/form-data" method="POST">
                                        <ul>
                                            <li>
                                                <label>Select Category</label>
                                                <select name="productCategory" id="productCategory">
                                                    <option value="">Select Category</option>
                                                    <% for(var i=0; i<result.length; i++)
                                                    {%>
                                                        <option value="<%=result[i]['category']%>"><%=result[i]['category']%></option>
                                                    <%}%>
                                                    
                                                </select>
                                            </li>
                                            <li>
                                                <label>Product Name</label>
                                                <input class="form-control" type="text" name="productName" id="productName" placeholder="Product Name">
                                            </li>
                                            
                                            <li>
                                                <label>Product Image</label>
                                                <input class="form-control" type="file" name="file" id="file">
                                            </li>
                                            
                                            <li>
                                                <label>Select Location</label>
                                                <input class="form-control" type="text" name="productLocation" id="productLocation" placeholder="Select Location">
                                            </li>

                                            <li>
                                                <div class="mapouter"><div class="gmap_canvas"><iframe width="600" height="500" id="gmap_canvas" src="https://maps.google.com/maps?q=university%20of%20san%20francisco&t=&z=13&ie=UTF8&iwloc=&output=embed" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"></iframe><a href="https://www.whatismyip-address.com/nordvpn-coupon/">nordvpn discount</a></div><style>.mapouter{position:relative;text-align:right;height:500px;width:600px;}.gmap_canvas {overflow:hidden;background:none!important;height:500px;width:600px;}</style></div>
                                            </li>
                                            <li class="full"><label>Description</label><textarea name="productDescription" class="ckeditor" id="productDescription" cols="30" rows="10"></textarea></li>
                                            <li class="full"><input type="submit" value="Add Product"> </li>
                                        </ul>
                                    </form>
                                </div>
                                <div class="clearfix"></div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>

        </div>