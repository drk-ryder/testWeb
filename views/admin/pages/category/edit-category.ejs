<div class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="header">
                                <h4 class="title">Edit Category</h4>
                            </div>
                            <div class="content">
                                <div class="payment-list">
                                    <form action="/admin/edit-category" enctype="multipart/form-data" method="POST">
                                        <ul>
                                           
                                            <li>
                                                <label>Category Name</label>
                                                <input class="form-control" type="text" id="category_name" name="category_name" value="<%=result[0]['category']%>" placeholder="Type Category Name">
                                            </li>
                                            <input type="hidden" name="id" value="<%=result[0]['id']%>">
                                          
                                            <li>
                                                <label>Select Category image</label>
                                                <input class="form-control" type="file" id="file" name="file">
                                            </li>
                                          
                                            <li class="full"><input type="submit" value="Update"> </li>
                                        </ul>
                                    </form>
                                </div>
                                <div class="clearfix"></div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>

        </div>